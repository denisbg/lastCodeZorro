  <body style="font-size: 14px; line-height: 18px; word-spacing: normal;">
    <div style>
      <!--[if mso | IE]><table align="center" border="0" cellpadding="0" cellspacing="0" class="" role="presentation" style="width:600px;" width="600" ><tr><td style="line-height:0px;font-size:0px;mso-line-height-rule:exactly;"><![endif]-->
      <div style="margin:0px auto;max-width:600px;">
        <table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" style="width:600px;">
          <tbody>
            <tr>
              <td style="direction:ltr;font-size:0px;padding:40px 0px;text-align:center;">
                <!--[if mso | IE]><table role="presentation" border="0" cellpadding="0" cellspacing="0"><tr><td class="" width="600px" ><table align="center" border="0" cellpadding="0" cellspacing="0" class="" role="presentation" style="width:600px;" width="600" bgcolor="#FFFFFF" ><tr><td style="line-height:0px;font-size:0px;mso-line-height-rule:exactly;"><![endif]-->
                <table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" style="background:#FFFFFF;background-color:#FFFFFF;width:600px;" width="600">
                  <tbody>
                    <tr>
                      <td style="border:1px solid #E7E7E7;direction:ltr;font-size:0px;padding:5px;text-align:center;">
                        <table role="presentation" border="0" cellpadding="0" cellspacing="0">

                          <!-- Start Header -->
                          {% block header %}
                            <tr>
                              <td class="" style="vertical-align:top;width:588px;">
                                <table border="0" cellpadding="0" cellspacing="0" role="presentation" style="background-color:#F5F5F5;vertical-align:top; width: 588px;" width="588px">
                                  <tbody>
                                    <tr>
                                      <td align="center" class="default-logo" style="font-size:0px;padding:16px 0;word-break:break-word;">
                                        <table border="0" cellpadding="0" cellspacing="0" role="presentation" style="border-collapse:collapse;border-spacing:0px;">
                                          <tbody>
                                            <tr>
                                              <td style="width:112px;">
                                                <a href="#" target="_blank" style="text-decoration: none;">
                                                  <img alt height="auto" src="{{ domaine }}images/email/logo.png" style="border:0;display:block;outline:none;text-decoration:none;height:auto;width:100%;font-size:13px;" width="112"/>
                                                </a>
                                              </td>
                                            </tr>
                                          </tbody>
                                        </table>
                                      </td>
                                    </tr>
                                  </tbody>
                                </table>
                              </td>
                            </tr>
                          {% endblock %}
                          <!-- End Header -->

                          <!-- Start Content -->
                          {% block content %}{% endblock %}
                          <!-- End Content -->

                          <!-- Start Footer -->
                          {% block footer %}
                            <tr>
                              <td class="" style="vertical-align:top;width:588px;">
                                <table border="0" cellpadding="0" cellspacing="0" role="presentation" width="588px">
                                  <tbody>
                                    <tr>
                                      <td style="vertical-align:top;padding:30px 23px;">
                                        <table border="0" cellpadding="0" cellspacing="0" role="presentation" style width="100%">
                                          <tbody>
                                            <tr>
                                              <td align="left" style="font-size:0px;padding:0 0 15px;word-break:break-word;">
                                                <div style="font-family:Montserrat;font-size:14px;font-weight:500;line-height:18px;text-align:left;color:#000000;">
                                                  {% if textFooter is defined %}
                                                    {{textFooter}}
                                                  {% else %}
                                                      Bien cordialement,
                                                  {% endif %}                          
                                                </div>
                                              </td>
                                            </tr>
                                            <tr>
                                              <td align="left" style="font-size:0px;padding:0;word-break:break-word;">
                                                <div style="font-family:Montserrat;font-size:14px;font-weight:600;line-height:18px;text-align:left;color:#000000;">
                                                  L’équipe Fingz                                
                                                </div>
                                              </td>
                                            </tr>
                                          </tbody>
                                        </table>
                                      </td>
                                    </tr>
                                  </tbody>
                                </table>
                              </td>
                            </tr>
                          {% endblock %}
                        <!-- End Footer -->

                        </table>
                      </td>
                    </tr>
                  </tbody>
                </table>
              <!--[if mso | IE]></td></tr></table></td></tr></table><![endif]-->
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    <!--[if mso | IE]></td></tr></table><![endif]-->
    </div>
  </body>

</html>
