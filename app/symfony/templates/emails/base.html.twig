    <style type="text/css">
    @media(max-width: 575px) {
      body {
        padding: 0 10px;
      }

      .img-product {
        margin-bottom: 15px;
      }

      .table-responsive td {
        display: block;
      }

      .bloc-tarif {
        display: block !important;
        width: calc(100% - 46px) !important;
        margin: auto;
        float: none !important;
      }
    }

    @media (prefers-color-scheme: dark) {
      body {
        background: #000;
      }
    }

